        </div> <!--//.posts-box  -->
    </div>
    <div class="col-lg-4 sidebar sidebar--right u-md-down-margin-t-40">
        <div class="sidebar__inner" data-sticky_column="">
            <div class="widget widget--transparent-box img-banner">
                <figure>
                    <div class="m-post-content m-post-content--nyc">
                        <b>@ListenAndWrite.Web.Resources.HomeTexts.Level</b>
                        <div class="post-tags">
                            <div class="tags-wrap">
                                @foreach (var level in Model)
                                {
                                    <a style="font-size: 80%;" href="/audio/level/@(level)">@(level)</a>
                                }

                            </div>
                        </div>
                    </div>
                </figure>
            </div>
            <div class="widget widget--transparent-box img-banner">
                <figure>

                </figure>
            </div>

        </div>
    </div>
</div>
<script id="data-template" type="x-tmpl-mustache">
    <article class="post-item">
        <div class="post-content">
            <div class="post-meta">
                <h6 class="post-title">
                    <a href="/audio/choose/{{Id}}">{{AudioTitle}}</a>
                </h6>
                <div class="item">
                </div>
                <div class="item">
                    <i class="fa fa-star-half-o"></i>level = {{Level}}
                </div>
                <div class="item">
                    <i class="fa fa-hourglass-o"></i>{{Duration}}
                </div>
                <div class="item">
                    <time><i class="fa fa-clock-o"></i>{{CreatedDate}}</time>
                </div>
            </div>
        </div>
    </article>
    <hr />
</script>