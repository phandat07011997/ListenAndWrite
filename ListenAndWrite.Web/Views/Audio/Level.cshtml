@model IEnumerable<int>
@{
    ViewBag.Title = "Level";
    Layout = "~/Views/Shared/Layouts.cshtml";
}
@section footerJS{
    <script src="~/Assets/Admin/libs/mustache/mustache.min.js"></script>
    <script src="~/Assets/Client/js/jquery.twbsPagination.min.js"></script>
    <script src="~/Assets/Client/js/controllers/listAudio.js"></script>
    <script async src="~/Assets/Client/pagead2.googlesyndication.com/pagead/js/f.txt"></script>
    <!-- index middle text only -->
    <ins class="adsbygoogle"
         style="display:inline-block;width:234px;height:60px"
         data-ad-client="ca-pub-0119806589085646"
         data-ad-slot="2273830705"></ins>
    <script>
        (adsbygoogle = window.adsbygoogle || []).push({});
    </script>
}

<div class="row" data-sticky_parent="">
    <div class="col-lg-8 content-wrapper">
        <div class="posts-box3 archive-box">
            <div class="posts-box3__top">
                <h2 class="posts-box3__top__title">@ListenAndWrite.Web.Resources.HomeTexts.LatestAudio: @(ViewBag.Level != null ? " "+ @ListenAndWrite.Web.Resources.HomeTexts.Level+" " + ViewBag.Level : "") </h2>

            </div>
            <input type="hidden" id="level" value="@ViewBag.Level" />

            <div class="archive-posts">


            </div><!--//.posts-list -->
            <ul id="pagination" class="pagination-sm"></ul>
